@model IEnumerable<System.Collections.Generic.KeyValuePair<string, string>>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Configuration</title>
    <style>
        table { border-collapse: collapse; width: 100%; }
        th, td { border: 1px solid #ccc; padding: 8px; text-align: left; }
        th { background: #f4f4f4; }
    </style>
</head>
<body>
    <h1>Configuration Key-Value Pairs</h1>
    <table>
        <thead>
            <tr>
                <th>Key</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var kv in Model)
            {
                <tr>
                    <td>@kv.Key</td>
                    <td>@kv.Value</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>